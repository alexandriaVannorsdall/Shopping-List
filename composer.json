{
    "name": "lindyhopchris/php-shopping-list",
    "description": "A simple shopping list application",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "Unlicensed",
    "autoload": {
        "psr-4": {
            "Lindyhopchris\\ShoppingList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
